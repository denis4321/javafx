<StackPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxapplication1.fxml.MyController">
    <Rectangle fx:id="rectangle" arcHeight="20" arcWidth="20" fill="lightskyblue" width="400" height="60">
        <effect>
            <DropShadow fx:id="dropshadow" color="GRAY" offsetY="3.0"/>                
        </effect>
    </Rectangle>
    <Text fx:id="text" fill="RED" effect="$dropshadow" style="-fx-font-weight:bold; -fx-font-size:32" text="Java FX!">
        
    </Text>
</StackPane>
